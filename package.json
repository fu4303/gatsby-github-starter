{
  "name": "gatsby-github-starter",
  "description": "GatsbyJS starter feat. Github GraphQL v4 / TypeScript / Emotion / React Spring",
  "version": "1.0.0",
  "author": "Luke Secomb <hello@lukesecomb.digital>",
  "keywords": [
    "gatsby",
    "github",
    "graphql"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^10.0.11",
    "babel-plugin-styled-components": "^1.7.1",
    "classnames": "^2.2.6",
    "gatsby": "^3.4.2",
    "gatsby-image": "^3.0.0",
    "gatsby-plugin-canonical-urls": "^3.0.0",
    "gatsby-plugin-emotion": "^6.0.0",
    "gatsby-plugin-google-analytics": "^3.0.0",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^3.0.1",
    "gatsby-plugin-typescript": "^3.0.0",
    "gatsby-remark-copy-linked-files": "^4.0.0",
    "gatsby-remark-images": "^5.0.0",
    "gatsby-remark-prismjs": "^5.0.0",
    "gatsby-remark-responsive-iframe": "^4.0.0",
    "gatsby-remark-smartypants": "^4.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-source-graphql": "^3.0.0",
    "gatsby-transformer-json": "^3.0.0",
    "gatsby-transformer-remark": "^4.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "modern-normalize": "^1.0.0",
    "normalize.css": "^8.0.0",
    "polished": "^4.0.5",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-spring": "^8.0.20",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/classnames": "2.3.0",
    "@types/node": "14.17.4",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-helmet": "6.1.1",
    "gh-pages": "3.2.3",
    "prettier": "2.3.1",
    "rimraf": "3.0.2",
    "tslint": "6.1.3",
    "tslint-config-blvd": "1.2.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0"
  }
